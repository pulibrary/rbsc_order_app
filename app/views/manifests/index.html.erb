<p id="notice"><%= notice %></p>

<h1>Listing Manifests</h1>

<table>
  <thead>
    <tr>
      <th>Call num</th>
      <th>Label</th>
      <th>Memo</th>
      <th>Url</th>
      <th>Uri</th>
      <th>Item qty</th>
      <th>Division</th>
      <th>Format</th>
      <th>Permissions</th>
      <th>Start item</th>
      <th>End item</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manifests.each do |manifest| %>
      <tr>
        <td><%= manifest.call_num %></td>
        <td><%= manifest.label %></td>
        <td><%= manifest.memo %></td>
        <td><%= manifest.url %></td>
        <td><%= manifest.uri %></td>
        <td><%= manifest.item_qty %></td>
        <td><%= manifest.division %></td>
        <td><%= manifest.format %></td>
        <td><%= manifest.permissions %></td>
        <td><%= manifest.start_item %></td>
        <td><%= manifest.end_item %></td>
        <td><%= link_to 'Show', manifest %></td>
        <td><%= link_to 'Edit', edit_manifest_path(manifest) %></td>
        <td><%= link_to 'Destroy', manifest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manifest', new_manifest_path %>
